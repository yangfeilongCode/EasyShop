<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="feicui.edu.easyshop">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--加载页面-->
        <activity android:name=".LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--主页面-->
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".user.login.LoginActivity"/>
        <activity android:name=".user.register.RegisterActivity"/>
        <activity android:name=".user.MeDetailtActivity"/>
        <activity android:name=".user.SetPetNameActivity"/>
    </application>

</manifest>